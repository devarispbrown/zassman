<div class="container bodypadding">
  <h1 class="tk-proxima-nova">Search Results</h1>

  <% @search_results.each do |search| %>
    <div class="results">
      <div class="box">
        <%= link_to (image_tag search.image(:medium)), asset_path(search.id) %>
      </div>
    </div>

  <% end %>
</div>